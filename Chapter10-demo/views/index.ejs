<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.24.0/axios.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js" crossorigin="anonymous"></script>
</head>
<body>
    <div>
        <form method="post" action="/admin">
            <input type="text" placeholder="name" name="name" id="name">
            <input type="number" placeholder="age" name="age" id="age">
            <button type="submit">Submit</button>
        </form>
    </div>
    <div>
        <input type="text" placeholder="name" name="name" id="updateName">
        <input type="number" placeholder="age" name="age" id="updateAge">
        <button onclick="updateData()">Update</button>
    </div>
    <div>
        <input type="text" placeholder="name" name="name" id="deleteName">
        <button onclick="deleteData()">Delete</button>
    </div>

    <script>
        const updateData = (()=>{
            let updateData = {}
            updateData.name = $('#updateName').val()
            updateData.age = $('#updateAge').val()
            axios.patch('/admin', updateData);
        })
        const deleteData = (()=>{
            let deleteData = {}
            deleteData.name = $('#deleteName').val()
            axios.delete('/admin', {data:{data:deleteData}});
        })
    </script>
</body>
</html>